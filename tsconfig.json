{
  "compilerOptions": {
    "noImplicitAny": true,
    "jsx": "preserve",
    "removeComments": false,
    "target": "es6",
    "module": "es6",
    "outDir": "src_es6",
    "inlineSources": true,
    "inlineSourceMap": true,
	"forceConsistentCasingInFileNames": true,
	"listFiles": true,
	"noImplicitReturns": false,
	"pretty": false,
	"newLine": "lf"
  },
  "compileOnSave": false,
  "exclude": ["node_modules"]
}
